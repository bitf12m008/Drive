@model IEnumerable<DriveSafe_Server.TripLog>
@{
    ViewBag.Title = "ViewDetail";
    Layout = "~/Views/Shared/UserProfileLayout.cshtml";
}



    <h1 style="color:black;text-align:center">Trip Detail</h1>
    <br /><br />


    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-10">
            <div style="float:left">
                <table class="table table-striped table-bordered table-hover table-condensed table-responsive ">
                    <thead>
                        <tr>
                            <th> Date</th>
                            <th> TripID </th>
                            <th>Time</th>
                            <th>Speed</th>
                            <th>RPM</th>
                            <th>Voltage</th>
                            <th>CoolantTemp</th>
                            <th>Latitude</th>
                            <th>Longitude</th>
                            <th>High Speed</th>
                            <th>Rapid Acceleration</th>
                            <th>Harsh Brake</th>
                            <th>RPM Violation</th>
                            <th>CoolantTemp Violation</th>

                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var p in Model)
                        {
                            <tr style="height:20%;">
                                <td>@p.Date</td>
                                <td>@p.TripID</td>

                                <td>@p.Time</td>

                                <td>@p.Speed KM/H</td>
                                <td>@p.RPM</td>
                                <td>@p.Voltage V</td>
                                <td>@p.CoolantTemp</td>
                                <td>@p.Latitude</td>
                                <th>@p.Longitude</th>
                                <th>@p.SpeedViolation</th>
                                <th>@p.OverAccelViolation</th>
                                <th>@p.HarshBrakeViolation</th>
                                <th>@p.RpmViolation</th>
                                <th>@p.CoolTempViolation</th>
                                


                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>



    <br /><br /><br />

